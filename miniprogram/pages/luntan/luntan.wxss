/* pages/list/list.wxss */
.page {
  position: relative;
}
.add {
  position: absolute;
  right: 58rpx;
  bottom: 58rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
  background: #b3fbdb;
  border: 1px solid rgb(185, 117, 117);
  border-radius: 50%;
}
.add image {
  width: 48rpx;
  height: 48rpx;
}
page,.page {
  width: 100%;
  height: 100%;
  font-family: Helvetica, 'Hiragino Sans GB', 'Microsoft Yahei', '微软雅黑', Arial, sans-serif;
  background-image: url("https://z3.ax1x.com/2021/05/28/2knyZD.jpg");
  background-size: cover;
}
.tview{
  display: flex;
  width:90%;
  margin-left: 40rpx;
  margin-top: 10rpx;
  flex-direction: column;
  background-color: #e2ffe8;
  border-radius: 40rpx;
  justify-content: center;  
  text-align: center;
}
.add1 {
  position: absolute;
  right: 200rpx;
  bottom: 58rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
  background: #b3fbdb;
  border: 1px solid rgb(185, 117, 117);
  border-radius: 50%;
}
